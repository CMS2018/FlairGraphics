
@using DeveloperCRM.Base.Models;

@{
    Layout = null;

}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <meta charset="utf-8" />
    <title>Agency CRM</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, shrink-to-fit=no" />
    @*<link rel="apple-touch-icon" href="pages/ico/60.png">
        <link rel="apple-touch-icon" sizes="76x76" href="pages/ico/76.png">
        <link rel="apple-touch-icon" sizes="120x120" href="pages/ico/120.png">
        <link rel="apple-touch-icon" sizes="152x152" href="pages/ico/152.png">*@
    <link rel="icon" type="image/x-icon" href="~/favicon.ico" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-touch-fullscreen" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="default">
    <meta content="" name="description" />
    <meta content="" name="author" />
    <link href="@AWSBaseURL/Content/assets/plugins/pace/pace-theme-flash.css" rel="stylesheet" type="text/css" />
    <link href="@AWSBaseURL/Content/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="@AWSBaseURL/Content/assets/plugins/font-awesome/css/font-awesome.css" rel="stylesheet" type="text/css" />
    <link href="@AWSBaseURL/Content/assets/plugins/jquery-scrollbar/jquery.scrollbar.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="@AWSBaseURL/Content/assets/plugins/select2/css/select2.min.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="@AWSBaseURL/Content/assets/plugins/switchery/css/switchery.min.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="@AWSBaseURL/Content/pages/css/pages-icons.css" rel="stylesheet" type="text/css">
    <link class="main-stylesheet" href="@AWSBaseURL/Content/pages/css/pages.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript">
        window.onload = function () {
            // fix for windows 8
            if (navigator.appVersion.indexOf("Windows NT 6.2") != -1)
                document.head.innerHTML += '<link rel="stylesheet" type="text/css" href="@AWSBaseURL/Content/pages/css/windows.chrome.fix.css" />'
        }
    </script>
</head>
<body class="fixed-header ">
  
        <!-- START Login Background Pic Wrapper-->
        
        <div class="row">
            <div class="col-12">
                <div class="card-box">

                    <div class="col-sm-6 offset-3">
                        <div class="text-center p-t-10">
                            <h1 class="text-error m-t-50 p-t-10">500</h1>
                            <h2 class="text-danger m-t-30">Internal Server Error</h2>
                            <p class="text-muted m-t-30">



                                Why not try to return login page? or you can contact <a href="mailto:support@snagtick.com" class="text-primary"><b>support@snagtick.com</b></a>
                            </p>

                            <a class="btn btn-md btn-block btn-primary waves-effect waves-light m-t-20" href="~/Account/Login"> Return Login</a>

                            @if (DeveloperCRM.Base.Models.STUtil.GetWebConfigValue("IS_UAT") == "Y")
                            {
                                <p class="text-muted m-t-30">


                                    <hr />

                                    @if (Model != null)
                                    {
                                        @Html.Raw(Model.error_message);
                                    }
                                    else
                                    {
                                        Exception exception = Server.GetLastError();
                                        if (exception != null)
                                        {
                                            if (exception.InnerException != null)
                                            {
                                                @Html.Raw(exception.InnerException.ToString())
                                            }
                                        }
                                    }
                                </p>
                            }
                        </div>

                    </div>
                </div>
            </div>
        </div>
         





 

    <div id="divApplicationMessage" class="alertDivPosition">




    </div>
    <div id="loading" class="loadingGlobalVisiable">
        <div class="loadingImg">
            <img src=@Url.Content("~/Base/content/images/windows8.gif") border="0" />
            <p style="text-align: center; font-size:13px; color:#0094ff;"><strong>Loading...</strong></p>

        </div>

    </div>
    <!-- BEGIN VENDOR JS -->
    <script src="@AWSBaseURL/Content/assets/plugins/pace/pace.min.js" type="text/javascript"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/jquery/jquery-1.11.1.min.js" type="text/javascript"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/modernizr.custom.js" type="text/javascript"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/tether/js/tether.min.js" type="text/javascript"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/jquery/jquery-easy.js" type="text/javascript"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/jquery-unveil/jquery.unveil.min.js" type="text/javascript"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/jquery-ios-list/jquery.ioslist.min.js" type="text/javascript"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/jquery-actual/jquery.actual.min.js"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/jquery-scrollbar/jquery.scrollbar.min.js"></script>
    <script type="text/javascript" src="@AWSBaseURL/Content/assets/plugins/select2/js/select2.full.min.js"></script>
    <script type="text/javascript" src="@AWSBaseURL/Content/assets/plugins/classie/classie.js"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/switchery/js/switchery.min.js" type="text/javascript"></script>
    <script src="@AWSBaseURL/Content/assets/plugins/jquery-validation/js/jquery.validate.min.js" type="text/javascript"></script>
    <!-- END VENDOR JS -->
    <script src="@AWSBaseURL/Content/pages/js/pages.min.js"></script>
    <script>
        $(function () {
            $('#form-resetPassword').validate();

        })
    </script>
    <!--DIGIMIND Custom JS -->
    <script src="~/Base/Content/js/DIGIMINDBase.js"></script>
    <script src="~/Base/Content/js/Base.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {


            jQuery('#loading').hide();


            base.showMessage('', '', '', '', 0);




        });


        $(document).ajaxStart(function () {
            jQuery('#loading').show(); // show the gif image when ajax starts
        }).ajaxStop(function () {
            jQuery('#loading').hide(); // hide the gif image when ajax completes
        });
    </script>
    <script type="text/javascript">
        function LoginID_isExist() {
            StUtil.IsExist({
                ctrlId: 'LoginID',
                controllerName: 'Json',
                actionName: 'IsLoginIDExist',
                parmVal: $('#LoginID').val()
            });
        }
    </script>
</body>
</html>




